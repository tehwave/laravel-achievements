{
    "name": "tehwave/laravel-achievements",
    "description": "Simple, elegant Achievements the Laravel way",
    "keywords": [
        "php",
        "laravel",
        "package",
        "achievements",
        "badges",
        "tehwave"
    ],
    "homepage": "https://github.com/tehwave/laravel-achievements",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter JÃ¸rgensen",
            "email": "peterchrjoergensen@gmail.com",
            "homepage": "https://peterchrjoergensen.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0|^8.1",
        "illuminate/console": "^5.8|^6.0|^7.0|^8.0|^9.0||^10.0",
        "illuminate/database": "^5.8|^6.0|^7.0|^8.0|^9.0||^10.0",
        "illuminate/filesystem": "^5.8|^6.0|^7.0|^8.0|^9.0||^10.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0|^9.0||^10.0",
        "ramsey/uuid": "^3.7|^4.1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.3|^7.0|^8.0",
        "phpunit/phpunit": "^7.5|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "tehwave\\Achievements\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tehwave\\Achievements\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "tehwave\\Achievements\\AchievementsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}